package com.nix.dnd5sim.entities;

public class TestNPC {
	
	private int id;
	private String name; 
	private int money;
	
	public TestNPC() {
		super();
	}
	public TestNPC(int id, String name, int money) {
		super();
		this.id = id;
		this.name = name;
		this.money = money;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}

}
